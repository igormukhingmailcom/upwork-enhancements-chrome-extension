{
    "manifest_version": 2,
    "name": "UpWorkEnhancements",
    "version": "0.1.0",
    "description": "Enhance UpWork interface",
    "icons": {
        "16": "images/logo.png",
        "32": "images/logo.png",
        "48": "images/logo.png"
    },
    "content_scripts": [{
        "matches": ["https://www.upwork.com/reports/available"],
        "css":["src/fix/reports/available/style.css"]
    },{
        "matches": ["https://www.upwork.com/reports/in-progress"],
        "css":["src/fix/reports/in-progress/style.css"],
        "js":[
            "src/vendor/jquery.min.js",
            "src/fix/reports/in-progress/script.js"
        ]
    }]
}
